#version 460
#extension GL_GOOGLE_include_directive : require

#include "common.comp"

layout(set = 0, binding = 0) buffer InputBuffer {
    int inputData[];
};

layout(set = 0, binding = 1) buffer OutputBuffer {
    int outputData[];
};

void main() {
    uint idx = gl_GlobalInvocationID.x;
    outputData[idx] = inputData[idx] / 2;
    outputData[idx] = asd();
}